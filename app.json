{
  "expo": {
    "name": "Pomodoro Timer",
    "slug": "pomodoro-timetracker-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6366F1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pomodoro.timetracker",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6366F1"
      },
      "package": "com.pomodoro.timetracker",
      "versionCode": 1,
      "permissions": [
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#6366F1"
        }
      ],
      "expo-secure-store",
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "4e34aeb7-e987-4444-a332-894d69a7aabb"
      }
    }
  }
}
